<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oopssinsa.model.dao.MenuMapper">
<!--    <select id="insertProduct" resultType="String">-->
<!--        select * from test1-->
<!--    </select>-->
    <select id="login" resultType="accountDto">
        select
            *
        from
            account
        where
            id = #{id}
    </select>
    <insert id="insertProduct">
        insert into
            product
        values (
        #{Id}, #{brandId}, #{name}, #{size}, #{color}, #{categoryId}, #{volume}
        )
    </insert>
    <insert id="ibRequest">
        insert into
            ib_detail
        values (
        #{Id}, #{manufactureId}, #{productId}, #{loginId}, #{quantity}
        )
    </insert>
</mapper>